{
  "name": "astu",
  "version": "0.0.1",
  "description": "A tool for generating AWS security tokens",
  "author": "Peter Borgstedt <peter.borgstedt@gmail.com",
  "license": "MIT",
  "homepage": "https://github.org/peter-borgstedt/astu#readme",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/peter-borgstedt/astu.git"
  },
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "publish": "npm run lint && npm run test && run build && npm publish && npm run cleanup",
    "cleanup": "rm -r dist",
    "start": "export TS_NODE_FILES=true && ts-node -r tsconfig-paths/register src/index.ts",
    "test": "export TS_NODE_FILES=true && ./node_modules/.bin/nyc ./node_modules/.bin/mocha -r ts-node/register -r test/*.ts",
    "lint": "./node_modules/tslint/bin/tslint **/*.ts",
    "lintfix": "./node_modules/tslint/bin/tslint --fix **/*.ts",
    "sort-package-dependencies": "npm remove -S dummy; npm remove -D dummy"
  },
  "dependencies": {
    "aws-sdk": "2.373.0",
    "chalk": "2.4.1",
    "commander": "2.19.0",
    "figlet": "1.2.1",
    "fmt": "1.1.0",
    "inquirer": "6.2.1",
    "ora": "3.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/figlet": "1.2.0",
    "@types/inquirer": "0.0.43",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.12",
    "@types/ora": "3.0.0",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "shebang-loader": "0.0.1",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.11.0",
    "typescript": "^3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-node-externals": "1.7.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
